			<item label="Ristretto">
				<action name="Execute">
					<command>ristretto</command>
				</action>
			</item>
			<item label="Screenshot">
				<action name="Execute">
					<command>scrot '%Y-%m-%d-%s_screenshot_$wx$h.jpg' -e 'mv $f ~/Screenshots/'</command>
				</action>
			</item>
			<item label="Screenshot in 5">
				<action name="Execute">
					<command>scrot -d 5 'ARCHLabs.png' -e  'mv $f ~/Screenshots/ ; mirage ~/Screenshots/$f'</command>
				</action>
			</item>
			<item label="Screenshot in 10">
				<action name="Execute">
					<command>scrot -d 10 'ARCHLabs.png' -e  'mv $f ~/Screenshots/ ; mirage ~/Screenshots/$f'</command>
				</action>
			</item>
			<item label="Viewnior">
				<action name="Execute">
					<command>viewnior</command>
				</action>
			</item>
		</menu>
