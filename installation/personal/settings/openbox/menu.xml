			<action name="Execute">
				<command>
                    termite
                </command>
			</action>
		</item>
		<item label="Run Program">
			<action name="Execute">
				<command>
                    gmrun
                </command>
			</action>
		</item>
		<item label="Kill Nemo">
			<action name="Execute">
				<execute>nemo --quit</execute>
			</action>
		</item>
		<separator/>
		<item label="Firefox">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		<item label="Thunar">
			<action name="Execute">
				<command>
